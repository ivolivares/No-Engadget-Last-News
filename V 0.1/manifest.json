{
    "manifest_version": 2,
    "name": "No Engadget Last News",
    "version": "0.1",

    "description": "Toggle last news from new Engadget design.",
    "icons": {
        "128": "icon_128.png"
    },
    "author": "Iv√°n Olivares R. (@ivolivares)",
    "permissions": [
    	"http://*.engadget.com/*"
	],
    "offline_enabled": false,
    "content_scripts": [
		{
		  "matches": [
		  	"http://*.engadget.com/*"
	  	],
          "js": ["jquery.min.js", "sc.min.js"]
		}
	]
}